
@{
    ViewBag.Title = "StudentDetails";
}

<h2>StudentDetails</h2>
<table border="1">
    <tr><th>ID</th><td>@Model.StudentId</td></tr>
    <tr><th>Name</th><td>@Model.Name</td></tr>
    <tr><th>Age</th><td>@Model.Age</td></tr>
    <tr><th>Email</th><td>@Model.Email</td></tr>
</table>

<br />
<a href="@Url.Action("Index", "Student")">Back</a>